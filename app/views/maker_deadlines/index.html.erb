<%- model_class = MakerDeadline -%>
<link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
<link rel="stylesheet" href="/resources/demos/style.css">
<script src="https://code.jquery.com/jquery-1.12.4.js"></script>
<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>

<div class="page-header">
  <h1>メーカー一覧　　　<%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>


<link type="text/css" rel="stylesheet"
  href="http://code.jquery.com/ui/1.10.3/themes/cupertino/jquery-ui.min.css" />
<script type="text/javascript"
  src="http://code.jquery.com/jquery-1.10.2.min.js"></script>
<script type="text/javascript"
  src="http://code.jquery.com/ui/1.10.3/jquery-ui.min.js"></script>


<p id="notice"><%= notice %></p>

<table id = "kibans">
  <thead>
    <tr>
      <th>id</th>
      <th>メーカー</th>
      <th>期限</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @makers.each do |maker| %>
      <tr>
        <td><%= maker.id %></td>
        <td><%= maker.メーカー %></td>
        <td><%= maker.期限 %>年間</td>
        <td><%= link_to 'Show', maker %></td>
        <td><%= link_to 'Edit', edit_maker_deadline_path(maker) %></td>
        <td><%= link_to 'Destroy', maker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>

  </tbody>
</table>
<br>

<%= link_to '基板リスト', kibans_path %>
<%= link_to 'メーカー登録', new_maker_deadline_path %>
